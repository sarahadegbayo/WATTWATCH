<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Community Forum</title>
    <script src="https://cdn.tailwindcss.com"></script>
    <link href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined" rel="stylesheet" />
    <script>
        tailwind.config = {
            theme: {
                extend: {
                    colors: {
                        primary: '#1A3C34',
                        primaryDark: '#14342B',
                    }
                }
            }
        }
    </script>

    <style>
        html {
            scroll-behavior: smooth;
        }

        .light-butter-bg {
            background-color: #fef5d4;
        }

        .material-symbols-outlined {
            font-variation-settings: 'FILL' 0, 'wght' 400, 'GRAD' 0, 'opsz' 48;
        }

        .container-80 {
            width: 70%;
            max-width: 1200px;
            margin: 0 auto;
        }

        .bg-baby-pink {
            background-color: #ffd6e5;
        }

        .bg-very-light-butter {
            background-color: #fff5e1;
        }

        .bg-light-grey {
            background-color: #f3f4f6;
        }

        .bg-sky-blue {
            background-color: #e0f2fe;
        }

        .bullet {
            display: inline-block;
            width: 4px;
            height: 4px;
            background: black;
            border-radius: 50%;
            margin: 0 6px;
        }

        .dotted-oval {
            border: 2px dotted #000;
            border-radius: 50%;
            position: absolute;
            left: 50%;
            top: 50%;
            transform: translate(-50%, -50%);
        }

        .oval-1 {
            width: 500px;
            height: 350px;
        }

        .oval-2 {
            width: 350px;
            height: 250px;
        }

        @media (max-width: 640px) {
            .container-80 {
                width: 90%;
            }

            .dotted-oval {
                display: none;
                /* Hide ovals on mobile for simplicity */
            }

            .oval-1,
            .oval-2 {
                display: none;
            }

            .join-community img {
                width: 10vw;
                height: 10vw;
                min-width: 40px;
                min-height: 40px;
            }
        }
    </style>
</head>

<body class="min-h-screen">

    <!-- Navigation Bar -->
    <nav class="bg-white shadow-md">
        <div class="max-w-7xl mx-auto px-4">
            <div class="flex justify-between items-center h-16 md:h-24">
                <!-- Large Logo at extreme left -->
                <div class="flex-shrink-0">
                    <img src="/images/logo2.png" class="w-32 h-auto md:w-40" />
                </div>

                <!-- Desktop Menu Items -->
                <div class="hidden md:flex md:space-x-6 md:flex-1 md:justify-center">
                    <a href="/"
                        class="text-gray-700 hover:text-gray-900 px-2 py-1 text-sm border-b-2 border-transparent hover:border-gray-300">Home</a>
                    <a href="/outagemap"
                        class="text-gray-700 hover:text-gray-900 px-2 py-1 text-sm border-b-2 border-transparent hover:border-gray-300">Outage
                        Map</a>
                    <a href="/reportoutage"
                        class="text-gray-700 hover:text-gray-900 px-2 py-1 text-sm border-b-2 border-transparent hover:border-gray-300">Report
                        Outage</a>
                    <a href="/community"
                        class="text-primary border-b-2 border-primary px-2 py-1 text-sm font-medium">Community</a>
                    <% if (isAuthenticated) { %>
                        <a href="/dashboard"
                            class="border-transparent text-gray-700 hover:border-gray-300 hover:text-gray-700 inline-flex items-center px-1 pt-1 border-b-2 text-sm font-medium whitespace-nowrap">
                            Dashboard
                        </a>
                        <% } %>
                </div>

                <!-- Auth Buttons - Desktop -->
                <div class="hidden md:flex items-center space-x-2 pr-2">
                    <% if (!isAuthenticated) { %>
                        <a href="/api/v1/auth/signup"
                            class="bg-primary hover:bg-primaryDark text-white px-3 py-1.5 rounded-md text-sm font-medium">Sign
                            Up</a>
                        <a href="/api/v1/auth/signin"
                            class="border border-black hover:border-gray-700 text-black hover:text-gray-700 px-3 py-1.5 rounded-md text-sm">Sign
                            In</a>
                        <% } else { %>
                            <form action="/api/v1/auth/signout" method="post" style="display:inline;">
                                <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                                <button type="submit"
                                    class="border border-black hover:border-gray-700 text-black hover:text-gray-700 px-3 py-1.5 rounded-md text-sm">Sign
                                    Out</button>
                            </form>
                            <% } %>
                </div>

                <!-- Mobile menu button -->
                <div class="md:hidden flex items-center pr-2">
                    <button type="button" onclick="document.getElementById('mobile-menu').classList.toggle('hidden')"
                        class="p-2 rounded-md text-gray-400 hover:text-gray-500 hover:bg-gray-100 focus:outline-none">
                        <span class="sr-only">Open main menu</span>
                        <svg class="h-6 w-6" xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24"
                            stroke="currentColor">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
                                d="M4 6h16M4 12h16M4 18h16" />
                        </svg>
                    </button>
                </div>
            </div>
        </div>

        <!-- Mobile Menu -->
        <div id="mobile-menu" class="hidden md:hidden bg-white shadow-lg">
            <div class="pt-2 pb-3 space-y-1">
                <a href="/"
                    class="bg-primary bg-opacity-10 text-primary block pl-3 pr-4 py-2 border-l-4 border-primary text-sm">Home</a>
                <a href="/outagemap"
                    class="text-gray-500 hover:bg-gray-50 hover:text-gray-700 block pl-3 pr-4 py-2 border-l-4 border-transparent text-sm">Outage
                    Map</a>
                <a href="/reportoutage"
                    class="text-gray-500 hover:bg-gray-50 hover:text-gray-700 block pl-3 pr-4 py-2 border-l-4 border-transparent text-sm">Report
                    Outage</a>
                <a href="/community"
                    class="text-gray-500 hover:bg-gray-50 hover:text-gray-700 block pl-3 pr-4 py-2 border-l-4 border-transparent text-sm">Community</a>
                <a href="/dashboard"
                    class="text-gray-500 hover:bg-gray-50 hover:text-gray-700 block pl-3 pr-4 py-2 border-l-4 border-transparent text-sm">Dashboard</a>
            </div>
            <!-- Auth Buttons - Mobile -->
            <div class="px-4 py-2 flex flex-col space-y-2">
                <% if (!isAuthenticated) { %>
                    <a href="/api/v1/auth/signup"
                        class="bg-primary hover:bg-primaryDark text-white px-4 py-2 rounded-md text-sm font-medium text-center">Sign
                        Up</a>
                    <a href="/api/v1/auth/signin"
                        class="border border-black hover:border-gray-700 text-black hover:text-gray-700 px-4 py-2 rounded-md text-sm text-center">Sign
                        In</a>
                    <% } else { %>
                        <form action="/api/v1/auth/signout" method="post" style="display:inline;">
                            <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                            <button type="submit"
                                class="border border-black hover:border-gray-700 text-black hover:text-gray-700 px-4 py-2 rounded-md text-sm text-center">Sign
                                Out</button>
                        </form>
                        <% } %>
            </div>
        </div>
    </nav>

